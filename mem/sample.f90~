program Mem

integer :: la, lb, lc, ld, le, lf
integer c(0:5)

call foo(la, lb)
call foo2(ld, lf)
call foo3(lc, le)

contains

subroutine foo(a, b)
  integer :: a, b
end subroutine

subroutine foo2(d, f)
  integer :: d, f
end subroutine

subroutine foo3(c, e)
  integer :: c, e
end subroutine


end program Mem
